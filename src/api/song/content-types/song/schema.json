{
  "kind": "collectionType",
  "collectionName": "songs",
  "info": {
    "singularName": "song",
    "pluralName": "songs",
    "displayName": "Song",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
